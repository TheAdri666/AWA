{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-plusplus": "off",
        "no-prototype-builtins": "off",
        "arrow-parens": "off",
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-underscore-dangle": "off",
        "no-await-in-loop": "off"
    }
}
